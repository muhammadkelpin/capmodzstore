function Select_Menu() {
read -p "Select From Options [1-19 or x] :  " Ltt
case $Ltt in
1) clear ; run ;;
2) clear ; reset ;;
3) clear ; autoreboot ;;
4) clear ; gotop ;;
5) clear ; speedtest ;;
6) clear ; xp ;;
7) clear ; nano /etc/banner.txt ;;
8) clear ; change-domain ;;
9) clear ; fixhap ;;
10) clear ; fixcert ;;
11) clear ; cf ;;
12) clear ; UDEPE ;;
13) clear ; Create_Bot_Notif ;;
14) clear ; clearcache ;;
15) clear ; clearlog ;;
16) clear ; delet-cache-file ;;
17) clear ; Info_Port ;;
18) clear ; neo ;;
19) clear ; sc ;;
20) clear ; menu ;;
21) clear ; update_sc ;;
x) exit ;;
*) m-ftr ;;
esac
}


Banner
Menu_Lambofgod
